import com.gammagamma.kas.domain.db.AddressId;

CREATE TABLE address (
    id INTEGER AS AddressId NOT NULL UNIQUE PRIMARY KEY,
    street TEXT NOT NULL,
    suite TEXT,
    city TEXT,
    zipcode TEXT
);

selectAll:
SELECT * FROM address;

insert:
INSERT INTO address (
    id,
    street,
    suite,
    city,
    zipcode
) VALUES (?, ?, ?, ?, ?);
